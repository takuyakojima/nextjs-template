{
  "name": "nextjs-template",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "run-p dev:*",
    "dev:next": "next dev",
    "dev:path": "pathpida --enableStatic --watch",
    "build": "pathpida --enableStatic && next build",
    "start": "next start",
    "test": "jest --no-cache",
    "update-snapshot": "jest --updateSnapshot",
    "type-check": "tsc --pretty --noEmit",
    "format": "run-s format:*",
    "format:prettier": "prettier --write ./src",
    "format:eslint": "eslint --ignore-path .gitignore --fix ./src",
    "lint": "run-p lint:*",
    "lint:src": "eslint --ext \".ts,.tsx\" --fix ./src",
    "lint:style": "stylelint \"src/**/*.{css,sass,scss}\" --fix",
    "prepare": "husky install",
    "lint-staged": "lint-staged"
  },
  "dependencies": {
    "@aspida/axios": "^1.7.1",
    "@aspida/swr": "^1.7.1",
    "axios": "^0.24.0",
    "next": "12.0.7",
    "normalize.css": "^8.0.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "swr": "^1.1.2-beta.0"
  },
  "devDependencies": {
    "@next/eslint-plugin-next": "^12.0.7",
    "@testing-library/jest-dom": "^5.16.1",
    "@testing-library/react": "^12.1.2",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.0.3",
    "@types/node": "17.0.0",
    "@types/react": "17.0.37",
    "@types/react-dom": "17.0.11",
    "@typescript-eslint/eslint-plugin": "^5.7.0",
    "@typescript-eslint/parser": "^5.7.0",
    "autoprefixer": "^10.4.0",
    "babel-jest": "^27.4.5",
    "eslint": "^8.5.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-import-resolver-typescript": "^2.5.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-react": "^7.27.1",
    "eslint-plugin-react-hooks": "^4.3.0",
    "husky": "^7.0.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^27.4.5",
    "jest-watch-typeahead": "^1.0.0",
    "lint-staged": "^12.1.3",
    "npm-run-all": "^4.1.5",
    "pathpida": "^0.17.0",
    "postcss": "^8.4.5",
    "postcss-flexbugs-fixes": "^5.0.2",
    "postcss-import": "^14.0.2",
    "postcss-preset-env": "^7.0.2",
    "prettier": "^2.5.1",
    "react-test-renderer": "^17.0.2",
    "sass": "^1.45.0",
    "stylelint": "^14.1.0",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-recommended": "^6.0.0",
    "stylelint-config-recommended-scss": "^5.0.2",
    "stylelint-declaration-block-no-ignored-properties": "^2.5.0",
    "stylelint-no-unsupported-browser-features": "^5.0.2",
    "stylelint-prettier": "^2.0.0",
    "stylelint-scss": "^4.0.1",
    "ts-jest": "^27.1.2",
    "typescript": "4.5.4"
  },
  "peerDependencies": {
    "postcss": "^8.4.5"
  }
}
