{
  "name": "nextjs-template",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "run-p dev:*",
    "dev:next": "next dev",
    "dev:path": "pathpida --enableStatic --watch",
    "build": "pathpida --enableStatic && next build",
    "start": "next start",
    "test": "jest --no-cache",
    "update-snapshot": "jest --updateSnapshot",
    "type-check": "tsc --pretty --noEmit",
    "format": "run-s format:*",
    "format:prettier": "prettier --write ./src",
    "format:eslint": "eslint --ignore-path .gitignore --fix ./src",
    "lint": "run-p lint:*",
    "lint:src": "eslint --ext \".ts,.tsx\" --fix ./src",
    "lint:style": "stylelint \"src/**/*.{css,sass,scss}\" --fix",
    "prepare": "husky install",
    "lint-staged": "lint-staged"
  },
  "dependencies": {
    "@aspida/axios": "^1.7.1",
    "@aspida/swr": "^1.7.1",
    "axios": "^1.2.1",
    "next": "13.0.6",
    "normalize.css": "^8.0.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "swr": "^2.0.0"
  },
  "devDependencies": {
    "@next/eslint-plugin-next": "^13.0.6",
    "@testing-library/jest-dom": "^5.16.2",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/jest": "^29.2.4",
    "@types/node": "18.11.13",
    "@types/react": "18.0.26",
    "@types/react-dom": "18.0.9",
    "@typescript-eslint/eslint-plugin": "^5.11.0",
    "@typescript-eslint/parser": "^5.11.0",
    "autoprefixer": "^10.4.13",
    "babel-jest": "^27.5.1",
    "eslint": "^8.9.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-import-resolver-typescript": "^3.5.2",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^8.0.2",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.3.1",
    "jest-environment-jsdom": "^29.3.1",
    "jest-watch-typeahead": "^2.2.1",
    "lint-staged": "^13.1.0",
    "npm-run-all": "^4.1.5",
    "pathpida": "^0.20.1",
    "postcss": "^8.4.19",
    "postcss-flexbugs-fixes": "^5.0.2",
    "postcss-import": "^15.1.0",
    "postcss-preset-env": "^7.3.3",
    "prettier": "^2.8.1",
    "react-test-renderer": "^18.2.0",
    "sass": "^1.49.7",
    "stylelint": "^14.5.0",
    "stylelint-config-prettier": "^9.0.4",
    "stylelint-config-recommended": "^9.0.0",
    "stylelint-config-recommended-scss": "^8.0.0",
    "stylelint-declaration-block-no-ignored-properties": "^2.6.0",
    "stylelint-no-unsupported-browser-features": "^6.0.1",
    "stylelint-prettier": "^2.0.0",
    "stylelint-scss": "^4.1.0",
    "ts-jest": "^29.0.3",
    "typescript": "4.9.4"
  },
  "peerDependencies": {
    "postcss": "^8.4.5"
  }
}
